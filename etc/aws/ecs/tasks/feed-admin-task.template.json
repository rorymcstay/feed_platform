{
    "containerDefinitions": [
        {
            "name": "feed-admin",
            "image": "${IMAGE_REPOSITORY}/feed/feed-admin:${COMPONENT_VERSION}",
            "memory": 500,
            "cpu": 256,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "feed-logs",
                    "awslogs-region": "${DEFAULT_REGION}",
                    "awslogs-stream-prefix": "feed-admin"
                }
            },
            "portMappings": [
                {
                    "protocol": "tcp",
                    "containerPort": 80,
                    "hostPort": 80
                }
            ],
            "environment": [
                {
                    "name": "BROWSER_BASE_PORT",
                    "value": "4444"
                },
                {
                    "name": "CONTAINER_TIMEOUT",
                    "value": "10"
                },
                {
                    "name": "DATABASE_NAME",
                    "value": "feeds"
                },
                {
                    "name": "DATABASE_PASS",
                    "value": "feeds"
                },
                {
                    "name": "DATABASE_PORT",
                    "value": "5432"
                },
                {
                    "name": "DATABASE_USER",
                    "value": "feeds"
                },
                {
                    "name": "DATABASE_HOST",
                    "value": "database.feed"
                },
                {
                    "name": "FLASK_PORT",
                    "value": "80"
                },
                {
                    "name": "KAFKA_ADDRESS",
                    "value": "kafka.feed:29092"
                },
                {
                    "name": "LEADER_START",
                    "value": "\"feed has started\""
                },
                {
                    "name": "LEADER_TEMPLATE",
                    "value": "leader"
                },
                {
                    "name": "LEADER_BASE_PORT",
                    "value": "9000"
                },
                {
                    "name": "LOG_LEVEL",
                    "value": "DEBUG"
                },
                {
                    "name": "MONGO_HOST",
                    "value": "mongo.feed:27017"
                },
                {
                    "name": "MONGO_PASS",
                    "value": "root"
                },
                {
                    "name": "MONGO_USER",
                    "value": "root"
                },
                {
                    "name": "NANNY_HOST",
                    "value": "nanny.feed"
                },
                {
                    "name": "NETWORK",
                    "value": "compose_default"
                },
                {
                    "name": "PARAMETER_DATABASE",
                    "value": "params"
                },
                {
                    "name": "FORM_DATABASE",
                    "value": "forms"
                },
                {
                    "name": "PARAM_STATS_DATABASE",
                    "value": "params_stats"
                },
                {
                    "name": "ROUTER_HOST",
                    "value": "router.feed"
                },
                {
                    "name": "COMMANDS_HOST",
                    "value": "commands.feed"
                },
                {
                    "name": "SUMMARIZER_HOST",
                    "value": "summarizer.feed"
                },
                {
                    "name": "SELENIUM_PORT",
                    "value": "4444"
                },
                {
                    "name": "WORKER_TIMEOUT",
                    "value": "3"
                },
                {
                    "name": "LOGLEVEL",
                    "value": "DEBUG"
                }
            ]
        }
    ],
    "networkMode": "awsvpc",
    "family": "feed-admin-task",
    "executionRoleArn": "arn:aws:iam::064106913348:role/FeedAdmin",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "memory": "512",
    "cpu": "256"
}
