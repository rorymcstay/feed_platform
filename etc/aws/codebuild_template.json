{
  "name": "${COMPONENT}",
  "source": {
    "type": "CODECOMMIT",
    "location": "https://git-codecommit.us-west-2.amazonaws.com/v1/repos/feed_${COMPONENT}",
    "gitCloneDepth": 0,
    "insecureSsl": true,
    "buildspec":"$CODEBUILD_SRC_DIR_platform/etc/aws/buildspec.yml"
  },
  "secondarySources": [{
    "type":"CODECOMMIT",
    "location": "https://git-codecommit.us-west-2.amazonaws.com/v1/repos/feed_platform",
    "sourceIdentifier": "platform"
  }],
  "artifacts": {
    "type": "NO_ARTIFACTS"
  },
  "cache": {
    "type": "NO_CACHE",
    "location": ""
  },
  "environment": {
    "type": "LINUX_CONTAINER",
    "image": "aws/codebuild/standard:1.0",
    "computeType": "BUILD_GENERAL1_SMALL",
    "environmentVariables": [
      {
        "name": "PROJECT_NAME",
        "value": "feed",
        "type": "PLAINTEXT"
      },
      {
        "name": "COMPONENT",
        "value": "${COMPONENT}",
        "type": "PLAINTEXT"
      },
      {
        "name": "IMAGE_REPOSITORY",
        "value": "064106913348.dkr.ecr.us-west-2.amazonaws.com",
        "type": "PLAINTEXT"
      },
      {
          "name": "GIT_URL",
          "value": "https://git-codecommit.us-west-2.amazonaws.com/v1/repos/",
          "type": "PLAINTEXT"
      }
    ],
    "privilegedMode": true
  },
  "serviceRole": "arn:aws:iam::064106913348:role/FeedAdmin",
  "timeoutInMinutes": 5,
  "tags": [
    {
      "key": "project",
      "value": "feed"
    },
    {
        "key": "component",
        "value": "${COMPONENT}"
    }
  ],
  "badgeEnabled": true
}
