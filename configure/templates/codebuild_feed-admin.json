{
  "timeoutInMinutes": 5,
  "name": "${COMPONENT}",
  "serviceRole": "arn:aws:iam::064106913348:role/car_app",
  "tags": [
    {
      "value": "feed",
      "key": "project"
    }
  ],
  "artifacts": {
    "type": "NO_ARTIFACTS"
  },
  "cache": {
    "type": "NO_CACHE",
    "location": ""
  },
  "badgeEnabled": true,
  "environment": {
    "computeType": "BUILD_GENERAL1_SMALL",
    "privilegedMode": true,
    "image": "aws/codebuild/standard:1.0",
    "type": "LINUX_CONTAINER",
    "environmentVariables": [
      {
        "type": "PLAINTEXT",
        "name": "PROJECT_NAME",
        "value": "feed"
      },
      {
        "type": "PLAINTEXT",
        "name": "COMPONENT",
        "value": "${COMPONENT}"
      }
    ]
  },
  "source": {
    "gitCloneDepth": 0,
    "type": "CODECOMMIT",
    "location": "ssh://git-codecommit.us-west-2.amazonaws.com/v1/repos/${COMPONENT}",
    "insecureSsl": true
  }
}